    "coverage": "nyc -s npm run test -t node && nyc report --reporter=html",
    "depcheck": "aegir dep-check -i @types/mocha -i @types/sinon -i nyc -i abort-controller -i rimraf -i ipfs-core-types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/js-ipfs-unixfs.git"
  },
  "keywords": [
    "IPFS"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipfs/js-ipfs-unixfs/issues"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=4.0.0"
  },
  "homepage": "https://github.com/ipfs/js-ipfs-unixfs#readme",
  "devDependencies": {
    "@types/mocha": "^8.2.1",
    "@types/sinon": "^9.0.10",
    "abort-controller": "^3.0.0",
    "aegir": "^30.3.0",
    "detect-node": "^2.0.4",
    "ipfs-core-types": "^0.3.0",
    "ipfs-unixfs-importer": "^6.0.1",
    "ipld": "^0.28.0",
    "ipld-dag-pb": "^0.22.1",
    "ipld-in-memory": "^7.0.0",
    "it-all": "^1.0.5",
    "it-buffer-stream": "^2.0.0",
    "it-first": "^1.0.6",
    "merge-options": "^3.0.4",
    "multicodec": "^2.1.0",
    "native-abort-controller": "^1.0.3",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "uint8arrays": "^2.1.2"
  },
  "dependencies": {
    "cids": "^1.1.5",
    "err-code": "^3.0.1",
    "hamt-sharding": "^2.0.0",
    "ipfs-unixfs": "^3.0.1",
    "it-last": "^1.0.5",
    "multihashing-async": "^2.1.0"
  },
  "types": "dist/src/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "eslintConfig": {
    "extends": "ipfs"
