{
  "name": "ipfs-unixfs-exporter",
  "version": "4.0.1",
  "description": "JavaScript implementation of the UnixFs exporter used by IPFS",
  "leadMaintainer": "Alex Potsides <alex.potsides@protocol.ai>",
  "main": "src/index.js",
  "browser": {
    "fs": false
  },
  "scripts": {
    "prepare": "aegir build --no-bundle",
    "test": "aegir test",
    "clean": "rimraf ./dist",
